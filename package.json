{
    "name": "clean-architecture-talk",
    "version": "0.0.1",
    "description": "Clean Architecture Talk: Introduction to Clean Architecture",
    "homepage": "https://github.com/MSC29/clean-architecture-talk",
    "main": "index.html",
    "license": "GNU GPL v3",
    "scripts": {
        "start": "npx http-server"
    },
    "author": {
        "name": "MaÃ«l Sicard-Cras",
        "email": "mael.sicardcras@gmail.com",
        "web": "https://www.linkedin.com/in/maelsicardcras"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/MSC29/clean-architecture-talk.git"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "keywords": [
        "reveal",
        "slides",
        "presentation",
        "clean-architecture"
    ],
    "overrides": {
        "chokidar": "3.5.3",
        "glob-parent": "6.0.2"
    },
    "browserslist": "> 2%, not dead",
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "parser": "@babel/eslint-parser",
        "parserOptions": {
            "sourceType": "module",
            "allowImportExportEverywhere": true,
            "requireConfigFile": false
        },
        "globals": {
            "module": false,
            "console": false,
            "unescape": false,
            "define": false,
            "exports": false
        },
        "rules": {
            "curly": 0,
            "eqeqeq": 2,
            "wrap-iife": [
                2,
                "any"
            ],
            "no-use-before-define": [
                2,
                {
                    "functions": false
                }
            ],
            "new-cap": 2,
            "no-caller": 2,
            "dot-notation": 0,
            "no-eq-null": 2,
            "no-unused-expressions": 0
        }
    },
    "dependencies": {
        "http-server": "^14.1.1",
        "reveal.js": "^4.5.0"
    }
}